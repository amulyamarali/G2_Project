{
    "manifest_version": 3,
    "version": "1.0.0",
    "name": "G2_project",
    "description": "This is a simple extension ",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "activeTab",
        "scripting", 
        "tabs"
      ],
    "host_permissions": [
        "http:///",
        "https:///"
    ],
    "icons":{
        "16": "image.png",
        "32": "image.png",
        "64": "image.png"
    }
}